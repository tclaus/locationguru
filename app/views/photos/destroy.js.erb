$('#photos').html("<%= j render 'photos_list' %>");
<% if @location.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@location.listing_name.blank? && !@location.photos.blank? && !@location.address.blank? %>
  $('#publish_button').attr('disabled', false);
<% else %>
  $('#publish_button').attr('disabled', true);
<% end %>
